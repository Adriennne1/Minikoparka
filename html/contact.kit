    <header class="hero-img" id="home">
        <div class="hero-text">
            <h1 data-aos="fade-left">Kontakt <br>Zduńska Wola</h1>
        </div>
        <div class="hero-shadow"></div>
    </header>


    <main>
        <h2 class="section__heading" data-aos="fade-up">Kontakt</h2>
        <div class="underline"></div>

        <form action="https://formsubmit.io/send/mateusz.kaczmarek1209@wp.pl" method="POST" class="form"
            id="contactForm">
            <div class="title-form">Formularz kontaktowy</div>
            <div class="form-row">
                <label for="name" class="form-label">Imię:</label>
                <input class="form-control" type="text" name="name" required id="name"
                    placeholder="Podaj swoje imię...">
            </div>
            <div class="form-row">
                <label for="email" class="form-label">Email:</label>
                <input class="form-control" type="email" name="email" required id="email"
                    placeholder="Podaj swój adres e-mail...">
            </div>
            <div class="form-row">
                <label for="message" class="form-label">Wiadomość:</label>
                <textarea class="form-control" name="message" required id="message"
                    placeholder="Podaj treść wiadomości..."></textarea>
            </div>
            <div class="form-message"></div>
            <div class="rodo"><input type="checkbox"> Wyrażam zgodę na przetwarzanie moich danych osobowych podanych w
                powyższym formularzu w celu zbierania danych przez "Usługi minikoparką Mateusz Kaczmarek" oraz przez
                podmioty trzecie.</div>
            <div class="form-row">
                <div class="btn">
                    <button class="main-button"><input value="wyślij" type="submit"></button>
                </div>
            </div>

        </form>

        <section class="contact-box">
            <h2 class="section__heading" data-aos="fade-up">Dane firmy</h2>
            <div class="underline"></div>

            <p class="contact__text">
            <ul class="contact-ul">
                <li class="contact-li">Usługi minikoparką Mateusz Kaczmarek</li>
                <li class="contact-li"> ul. Wschodnia 20</li>
                <li class="contact-li"> 98-220 Zduńska Wola</li>
                <a class="link-contact" href="tel:+48790275736"><img class="icons-contact"
                        src="src/img/icons/smartphone-white.svg">790-275-736</a>
                <a class="link-contact" href="mailto: mail@mail.com"><img class="icons-contact"
                        src="src/img/icons/mail-white.svg"> mail@mail.com</li></a>
            </ul>
            </p>

        </section>

        <section class="location-box">
            <h2 class="section__heading" data-aos="fade-up">Lokalizacja</h2>
            <div class="underline"></div>

            <div class="maps">
                <iframe
                    src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d3505.650750522607!2d18.938478142494358!3d51.58895658360252!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x471a5b859483afc3%3A0xfb2814e29fae8c59!2sUS%C5%81UGI%20MINIKOPARK%C4%84%20Mateusz%20Kaczmarek!5e0!3m2!1spl!2spl!4v1708554422286!5m2!1spl!2spl"
                    width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy"
                    referrerpolicy="no-referrer-when-downgrade"></iframe>
            </div>
        </section>

    </main>

